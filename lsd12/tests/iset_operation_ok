program x;
  function main() : void;
    var
      a iset;
      function addIset( var ens : iset, i:int ) : void;
      var begin
        add i to ens;
      end;
      function createset( var a : iset ): void;
      var 
        b int;
      begin
        read b;
        while( !(b = 0) ) do
          addIset(a, b);
          read b;
        od;
      end;
      function printset( var ens : iset ) : void;
      var 
        i int;
      begin
        i := min ens;
        while( i <= max ens ) do
          if( i in ens ) then
            write i;
          fi;
          i := i + 1;
        od;
      end;
    begin
      createset( a );
      write #a;
      write min a;
      write max a;
      {printset( a );}
    end;
  end;