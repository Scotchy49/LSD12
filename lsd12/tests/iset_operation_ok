program x;
  function main() : void;
    var
      a iset;
      b int;
      function addIset( a : iset, i:int ) : void;
      var begin
        add i to a;
      end;
    begin
      write #a;  {0}
      addIset(a, 2);
      write max a; {2}
      add 5 to a; 
      write max a; {5}
      write min a; {2}
      write #a;    {2}
      remove 5 from a;
      write max a; {2}
      write #a;  {1}
    end;
  end;