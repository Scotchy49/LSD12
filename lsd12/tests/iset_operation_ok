program x;
  function main() : void;
    var
      a iset;
      function addIset( var a : iset, i:int ) : void;
      var begin
        add i to a;
        write #a;
      end;
      function createset( var a : iset ): void;
      var 
        b int;
      begin
        read b;
        while( !(b = 0) ) do
          addIset(a, b);
          read b;
        od;
      end;
      function printset( var a : iset ) : void;
      var 
        b iset;
      begin
        while(0 < #a) do
          write min a;
          add min a to b;
          remove min a from a;
        od;
        while( 0 < #b ) do
          add min b to a;
          remove min b from b;
        od;
      end;
    begin
      createset( a );
      printset( a );
      printset( a );
    end;
  end;