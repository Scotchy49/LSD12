program fib;
  function main() : void;
  var 
  	a int;
    function fib( i : int ) : int;
    var begin
      if( i <= 1 ) then
        return 1;
      fi;
      return fib(i-1) + fib(i-2);
    end;
  begin
      a := 1;
      while( 0 < a) do
        read a;
        write fib(a);
      od;
  end;
end;