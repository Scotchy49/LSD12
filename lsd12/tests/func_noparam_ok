program x;
  function main() : void;
    var
      a int;
      function a() : int;
        var begin
          return a;
        end;
      function b() : int;
        var a int;
        function c() : int;
          var begin 
            a := a(); 
            return a; 
          end;
        begin
          return c();
        end;
    begin
      a := 1;
      write a();
      write b();
    end;
  end;