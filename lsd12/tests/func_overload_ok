program x;
  function main() : void;
    var
      function a(a: int, b:bool) : void;
        var begin 
            b := !b;
            write 1;
        end;
      function a(a: int, b:int) : void;
        var begin 
            b := -b;
            write 2;
        end;
    begin
      a(0, 1);    
      {EXPECT:2}
      a(0, true); 
      {EXPECT:1}
    end;
  end;