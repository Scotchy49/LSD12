program x;
  function main() : void;
    var
      function a(a: int, b:bool) : void;
        var begin 
            b := !b;
            write a;
        end;
      function a(a: int, b:int) : void;
        var begin 
            b := -b;
            write a;
        end;
    begin
      a(1, 1);
      a(2, true);
    end;
  end;