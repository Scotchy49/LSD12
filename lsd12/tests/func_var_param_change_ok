program x;
  function main() : void;
    var
      a int;
      b bool;
      function a(var a: int, var b:bool) : void;
        var begin 
          a := a+1;
          b := !b;
        end;
    begin
      a := 1;
      b := false;
      a(a, b);
      write a;
      if( b ) then
        write 1;
      else
        write 0;
      fi;
    end;
  end;