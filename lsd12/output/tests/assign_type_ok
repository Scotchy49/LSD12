;|  OP_ID (x) []
;|  next
;|  OP_FUNCTION [ -> main():v(-1,0)]
;|  |  OP_ID (main) [ -> main():v(-1,0)]
;|  |  next
;|  |  OP_FUNCTION_PARAMS [ -> main():v(-1,0)]
;|  |  next
;|  |  OP_FUNCTION_TYPE (v) [ -> main():v(-1,0)]
;|  |  next
;|  |  OP_FUNCTION_DECLBLOCK [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  OP_FUNCTION_VAR_DECL [ -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  OP_ID (a) [ -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  next
;|  |  |  |  OP_VAR_TYPE (i) [ -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  next
;|  |  |  OP_FUNCTION_VAR_DECL [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  OP_ID (b) [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  next
;|  |  |  |  OP_VAR_TYPE (i) [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  next
;|  |  OP_FUNCTION_BODY [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  OP_INSTRUCTION_LIST [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  OP_ASSIGN [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  |  OP_LEXPR (a) [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;|  |  |  |  |  next
;|  |  |  |  |  OP_LEXPR (b) [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;Generating pcode for node OP_PROGRAM []
ssp 2
ujp @begin
;Generating pcode for node OP_FUNCTION_DECLBLOCK [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
define @begin
;Generating pcode for node OP_INSTRUCTION_LIST [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
;Generating pcode for node OP_ASSIGN [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
lda i 0 0
;Generating pcode for node OP_LEXPR (b) [ -> b:i(0,1) -> a:i(0,0) -> main():v(-1,0)]
lda i 0 1
ind i
sto i
stp
